   top: "reduce_proposal_conv6"
	param {
		lr_mult: 1.0
	}
	param {
		lr_mult: 2.0
	}
   convolution_param{
	   num_output: 512
	   kernel_size: 1
	   pad: 0
	   stride: 1
	   weight_filler {
		 type: "gaussian"
		 std: 0.01
	   }
	   bias_filler {
		 type: "constant"
		 value: 0
	   }
	   engine: CAFFE
   }
}

layer {
   name: "relu_proposal_reduce_conv6"
   type: "ReLU"
   bottom: "reduce_proposal_conv6"
   top: "reduce_proposal_conv6"
}

layer {
   name: "proposal_cls_score_conv6"
   type: "Convolution"
   bottom: "reduce_proposal_conv6"
   top: "proposal_cls_score_conv6"
	param {
		lr_mult: 1.0
	}
	param {
		lr_mult: 2.0
	}
   convolution_param{
	   num_output: 16   # 2(bg/fg) * 4(anchors) 
	   kernel_size: 1
	   pad: 0
	   stride: 1
	   weight_filler {
		 type: "gaussian"
		 std: 0.01
	   }
	   bias_filler {
		 type: "constant"
		 value: 0
	   }
	   engine: CAFFE
   }

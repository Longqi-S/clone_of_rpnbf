#1113 added norm layer
# temperarily masked, unmask after getting scale range by running for a few images
#layer {
#  name: "conv4_3_norm"
#  type: "Normalize"
#  bottom: "conv4_3"
#  top: "conv4_3_norm"
#  norm_param {
#    across_spatial: false
#    scale_filler {
#      type: "constant"
#      value: 40  #1120: 20-->40
#    }
#    channel_shared: false
#  }
#}

layer {
   name: "conv_proposal_conv4"
   type: "Convolution"
   bottom: res3d  #"conv4_3_norm"
   top: "conv_proposal_conv4"
	param {
		lr_mult: 1.0
	}
	param {
		lr_mult: 2.0
	}
   convolution_param{
	   num_output: 512
	   kernel_size: 3
	   pad: 1
	   stride: 1
	   weight_filler {
		 type: "gaussian"
		 std: 0.01
	   }
	   bias_filler {
		 type: "constant"
		 value: 0
	   }
   }
}

# add some context information: atrous with hole_size = 2
layer {
   name: "conv_proposal2_conv4"
   type: "Convolution"
   bottom: "conv4_3_norm"
   top: "conv_proposal2_conv4"
	param {
		lr_mult: 1.0
	}
	param {
		lr_mult: 2.0
	}
   convolution_param{
	   num_output: 512
	   kernel_size: 3
	   pad: 2 #1
	   dilation: 2 #stride: 1
	   weight_filler {
		 type: "gaussian"
		 std: 0.01
	   }
	   bias_filler {
		 type: "constant"
		 value: 0
	   }
   }
}

layer {
  name: "concat_proposal_conv4"
  bottom: "conv_proposal_conv4"
  bottom: "conv_proposal2_conv4"
  top: "concat_proposal_conv4"
  type: "Concat"
  concat_param {
    axis: 1 # concat channels: 512+512 = 1024
  }
}

layer {
   name: "relu_proposal_concat_conv4"
   type: "ReLU"
   bottom: "concat_proposal_conv4"
   top: "concat_proposal_conv4"
}

layer {
   name: "reduce_proposal_conv4"
   type: "Convolution"
   bottom: "concat_proposal_conv4"
   top: "reduce_proposal_conv4"
	param {
		lr_mult: 1.0
	}
	param {
		lr_mult: 2.0
	}
   convolution_param{
	   num_output: 512
	   kernel_size: 1
	   pad: 0
	   stride: 1
	   weight_filler {
		 type: "gaussian"
		 std: 0.01
	   }
	   bias_filler {
		 type: "constant"
		 value: 0
	   }
   }
}

layer {
   name: "relu_proposal_reduce_conv4"
   type: "ReLU"
   bottom: "reduce_proposal_conv4"
   top: "reduce_proposal_conv4"
}

layer {
   name: "proposal_cls_score_conv4"
   type: "Convolution"
   bottom: "reduce_proposal_conv4" #"conv_proposal_conv4"
   top: "proposal_cls_score_conv4"
	param {
		lr_mult: 1.0
	}
	param {
		lr_mult: 2.0
	}
   convolution_param{
	   num_output: 6   # 2(bg/fg) * 3(anchors) 
	   kernel_size: 1
	   pad: 0
	   stride: 1
	   weight_filler {
		 type: "gaussian"
		 std: 0.01
	   }
	   bias_filler {
		 type: "constant"
		 value: 0
	   }
   }
}

layer {

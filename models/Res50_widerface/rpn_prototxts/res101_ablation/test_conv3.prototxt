# ============== 0424: proposal layer for conv3 (res3)

# normalize
layer {
  name: "res3_norm"
  type: "Normalize"
  bottom: "res3b3"
  top: "res3_norm"
  norm_param {
    across_spatial: false
    scale_filler {
      type: "constant"
      value: 22.6  #0424: 32->22.6
    }
    channel_shared: false
  }
}

layer {
   name: "res3_proposal"
   type: "Convolution"
   bottom: "res3_norm"
   top: "res3_proposal"
	param {
		lr_mult: 1.0
	}
	param {
		lr_mult: 2.0
	}
   convolution_param{
	   num_output: 512
	   kernel_size: 3
	   pad: 1
	   stride: 1
	   weight_filler {
		 type: "gaussian"
		 std: 0.01
	   }
	   bias_filler {
		 type: "constant"
		 value: 0
	   }
	   #engine: CAFFE
   }
}

layer {
   name: "relu_res3_proposal"
   type: "ReLU"
   bottom: "res3_proposal"
   top: "res3_proposal"
}

layer {
   name: "proposal_cls_score"
   type: "Convolution"
   bottom: "res3_proposal"
   top: "proposal_cls_score"
	param {
		lr_mult: 1.0
	}
	param {
		lr_mult: 2.0
	}
   convolution_param{
	   num_output: 14  #2(bg/fg) * 7(anchors)
	   kernel_size: 1
	   pad: 0
	   stride: 1
	   weight_filler {
		 type: "gaussian"
		 std: 0.01
	   }
	   bias_filler {
		 type: "constant"
		 value: 0
	   }
	   #engine: CAFFE
   }
}

layer {
   name: "proposal_bbox_pred"
   type: "Convolution"
   bottom: "res3_proposal"
   top: "proposal_bbox_pred"
	param {
		lr_mult: 1.0
	}
	param {
		lr_mult: 2.0
	}
   convolution_param{
	   num_output: 28  #4(coords) * 7(anchors)
	   kernel_size: 1
	   pad: 0
	   stride: 1
	   weight_filler {
		 type: "gaussian"
		 std: 0.01
	   }
	   bias_filler {
		 type: "constant"
		 value: 0
	   }
	   #engine: CAFFE
   }
}
# ========= end of res3 proposal =================

#-----------------------output------------------------

# to enable the calculation of softmax loss, we first reshape blobs related to SoftmaxWithLoss
layer {
   bottom: "proposal_cls_score"
   top: "proposal_cls_score_reshape"
   name: "proposal_cls_score_reshape"
   type: "Reshape"
   reshape_param{
	   shape {
			dim: 0 
			dim: 2
			dim: -1 
			dim: 0
		}
	}
}

layer {
   name: "proposal_cls_prob"
   type: "Softmax"
   bottom: "proposal_cls_score_reshape"
   top: "proposal_cls_prob"
}

